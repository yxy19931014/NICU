@charset "utf-8";
/*! normalize.css v3.0.3 | MIT License | github.com/necolas/normalize.css */
html {
  font-family: sans-serif;
  -ms-text-size-adjust: 100%;
  -webkit-text-size-adjust: 100%;
}
body {
  margin: 0;
  background: transparent;
}
article,
aside,
details,
figcaption,
figure,
footer,
header,
hgroup,
main,
menu,
nav,
section,
summary {
  display: block;
}
audio,
canvas,
progress,
video {
  display: inline-block;
  vertical-align: baseline;
}
audio:not([controls]) {
  display: none;
  height: 0;
}
[hidden],
template {
  display: none;
}
a {
  background-color: transparent;
}
a:active,
a:hover {
  outline: 0;
}
abbr[title] {
  border-bottom: 1px dotted;
}
b,
strong {
  font-weight: bold;
}
dfn {
  font-style: italic;
}
h1 {
  font-size: 2em;
  margin: 0.67em 0;
}
mark {
  background: #ff0;
  color: #000;
}
small {
  font-size: 80%;
}
sub,
sup {
  font-size: 75%;
  line-height: 0;
  position: relative;
  vertical-align: baseline;
}
sup {
  top: -0.5em;
}
sub {
  bottom: -0.25em;
}
img {
  border: 0;
}
svg:not(:root) {
  overflow: hidden;
}
figure {
  margin: 1em 40px;
}
hr {
  box-sizing: content-box;
  height: 0;
}
pre {
  overflow: auto;
}
code,
kbd,
pre,
samp {
  font-family: monospace, monospace;
  font-size: 1em;
}
button,
input,
optgroup,
select,
textarea {
  color: inherit;
  font: inherit;
  margin: 0;
}
button {
  overflow: visible;
}
button,
select {
  text-transform: none;
}
button,
html input[type="button"],
input[type="reset"],
input[type="submit"] {
  -webkit-appearance: button;
  cursor: pointer;
}
button[disabled],
html input[disabled] {
  cursor: default;
}
button::-moz-focus-inner,
input::-moz-focus-inner {
  border: 0;
  padding: 0;
}
input {
  line-height: normal;
}
input[type="checkbox"],
input[type="radio"] {
  box-sizing: border-box;
  padding: 0;
}
input[type="number"]::-webkit-inner-spin-button,
input[type="number"]::-webkit-outer-spin-button {
  height: auto;
}
input[type="search"] {
  -webkit-appearance: textfield;
  box-sizing: content-box;
}
input[type="search"]::-webkit-search-cancel-button,
input[type="search"]::-webkit-search-decoration {
  -webkit-appearance: none;
}
fieldset {
  border: 1px solid #c0c0c0;
  margin: 0 2px;
  padding: 0.35em 0.625em 0.75em;
}
legend {
  border: 0;
  padding: 0;
}
textarea {
  overflow: auto;
}
optgroup {
  font-weight: bold;
}
table {
  border-collapse: collapse;
  border-spacing: 0;
}
td,
th {
  padding: 0;
}
/*适配模块*/
@media (min-width: 320px) {
  html {
    font-size: 42.66666667px;
  }
}
@media (min-width: 360px) {
  html {
    font-size: 48px;
  }
}
@media (min-width: 375px) {
  html {
    font-size: 50px;
  }
}
@media (min-width: 384px) {
  html {
    font-size: 51.2px;
  }
}
@media (min-width: 412px) {
  html {
    font-size: 54.93333333px;
  }
}
@media (min-width: 414px) {
  html {
    font-size: 55.2px;
  }
}
@media (min-width: 424px) {
  html {
    font-size: 56.53333333px;
  }
}
@media (min-width: 480px) {
  html {
    font-size: 64px;
  }
}
@media (min-width: 540px) {
  html {
    font-size: 72px;
  }
}
@media (min-width: 720px) {
  html {
    font-size: 96px;
  }
}
@media (min-width: 750px) {
  html {
    font-size: 100px;
  }
}
/*样式处理*/
body,
form,
input,
button,
fieldset,
h1,
h2 {
  margin: 0;
  padding: 0;
  border: none;
}
html {
  height: 100%;
}
body {
  font-size: 14px;
  font-family: -apple-system, BlinkMacSystemFont, "PingFang SC", "Helvetica Neue", STHeiti, "Microsoft Yahei", Tahoma, Simsun, sans-serif;
}
*,
::before,
::after,
input[type="search"] {
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  margin: 0;
  padding: 0;
}
ul {
  list-style: none;
}
.clearfix::after,
.clearfix::before {
  content: "";
  display: block;
  visibility: hidden;
  height: 0;
  line-height: 0;
  clear: both;
}
.nextPre {
  width: 100%;
  display: flex;
  position: fixed;
  bottom: 0;
  z-index: 100;
  display: none;
}
.nextPre button {
  width: 50%;
  color: #fff;
  height: 0.8rem;
  background: #48bebb;
  border: 1px solid #fff;
}
.mui-backdrop {
  height: 100%;
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 998;
  background-color: rgba(0, 0, 0, 0.3);
}
#popover {
  width: 100%;
  background: #fff;
  padding: 0 0.2rem 0 0.2rem;
  position: fixed;
  top: 1rem;
  left: 0;
  z-index: 999;
  display: none;
}
#popover .search {
  width: 100%;
  height: 0.75rem;
  margin-top: 0.2rem;
}
#popover .search .search_box {
  width: 100%;
  height: 0.6rem;
  margin: 0 auto;
  background: #fff;
  position: relative;
}
#popover .search .search_box form {
  width: 100%;
  padding-right: 1.5rem;
  outline: none;
}
#popover .search .search_box form .mui-input-row {
  height: 0.58rem;
}
#popover .search .search_box form input {
  width: 100%;
  height: 0.58rem;
  border-radius: 0.3rem;
  background: #fff;
  text-align: left;
  outline: none;
  border: 1px solid #48bebb;
}
#popover .search .search_box .search_btn {
  width: 0.42rem;
  height: 0.42rem;
  position: absolute;
  background: url(../imgs/observation/search_select.png) no-repeat center / 0.41rem 0.41rem;
  top: 0.09rem;
  right: 1.7rem;
}
#popover .historyData {
  display: flex;
  flex-wrap: wrap;
  width: 100%;
  height: 5rem;
  overflow: auto;
  padding-bottom: 0.2rem;
}
#popover .historyData .singleBox {
  width: 33.33%;
  height: 0.7rem;
  margin-bottom: 0.2rem;
}
#popover .historyData .singleBox .single {
  width: 95%;
  height: 100%;
  background: #eee;
  border-radius: 3px;
  display: inline-block;
}
#popover .historyData .singleBox .single span {
  line-height: 0.7rem;
}
#popover .historyData .singleBox.selected .single span {
  color: #48bebb;
}
#popover .currentData .single span {
  color: #48bebb;
}
#popover .historyData .single span {
  color: #333;
}
#popover .pullBox {
  width: 100%;
  height: 0.8rem;
  background: #fff;
  position: relative;
}
#popover .pullBox span {
  width: 0.32rem;
  height: 0.18rem;
  background: url(../imgs/observation/pull.png) no-repeat center / 0.32rem 0.18rem;
  display: inline-block;
  position: absolute;
  left: 50%;
  margin-left: -0.32rem;
  top: 0.3rem;
}
.selectArea {
  width: 80%;
  left: 50%;
  margin-left: -40%;
  position: fixed;
  top: 2rem;
  z-index: 999;
  display: none;
  color: #fff;
}
.selectArea h2 {
  width: 100%;
  height: 0.8rem;
  line-height: 0.8rem;
  font-size: 0.36rem;
  font-weight: 400;
  background: #48bebb;
  padding-left: 0.2rem;
  border-radius: 10px 10px 0 0;
}
.selectArea .liCon {
  width: 100%;
  height: 5rem;
  background: #fff;
  overflow: auto;
}
.selectArea ul li {
  width: 100%;
  height: 0.8rem;
  line-height: 0.8rem;
  font-size: 0.32rem;
  background: #fff;
  padding: 0 0.2rem 0 0.2rem;
  color: #48bebb;
  position: relative;
}
.selectArea ul li .hook {
  font-size: 0.36rem;
  position: absolute;
  right: 10%;
  display: none;
}
.selectArea ul li .itemConName {
  width: 100%;
  height: 100%;
  display: inline-block;
  border-bottom: 1px solid #ccc;
}
.selectArea .nextOper {
  padding: 0;
  display: flex;
  border-radius: 10px;
}
.selectArea .nextOper button {
  width: 50%;
  background: #48bebb;
  border-radius: 0 0 10px 0;
}
.selectArea .nextOper #clear {
  border-right: 1px solid #fff;
  border-radius: 0 0 0 10px;
}
.observationBox header {
  width: 100%;
  height: 1rem;
  background: #48bebb;
  position: fixed;
  z-index: 1000;
}
.observationBox header h1 {
  width: 100%;
  text-align: center;
  font-size: 0.36rem;
  color: #fff;
  font-weight: bold;
  line-height: 1rem;
}
.observationBox header h1 .title_icon {
  margin-left: 0.1rem;
}
.observationBox header .leftBack {
  positon: absolute;
  width: 1.5rem;
  top: 0;
  left: 0.2rem;
  z-index: 1001;
}
.observationBox header .leftBack .text {
  font-size: 0.28rem;
  color: #fff;
  line-height: 1rem;
  cursor: pointer;
  display: inline-block;
  
}
.dataTime {
  width: 100%;
  height: 0.88rem;
  margin-top: 0.2rem;
  border-top: 1px solid #bebebe;
  border-bottom: 1px solid #bebebe;
  background: #fff;
  overflow: hidden;
}
.dataTime #currentDate {
  height: 0.88rem;
  line-height: 0.88rem;
  display: inline-block;
  padding-left: 0.2rem;
}
.dataTime .time {
  width: 5.1rem;
  float: right;
  height: 0.88rem;
  line-height: 0.88rem;
  position: relative;
}
.dataTime .time span {
  width: 1.7rem;
  height: 0.88rem;
  line-height: 0.88rem;
  position: absolute;
  top: 0;
  left: 0;
  cursor: pointer;
}
.dataTime .time span input {
  width: 100%;
  height: 100%;
  border: none;
  color: #48bebb;
  font-size: 0.28rem;
}
.dataTime .time span i {
  width: 100%;
  height: 100%;
  color: #ccc;
  font-size: 0.28rem;
  display: block;
  position: absolute;
  top: 0;
  left: 0;
  background: #fff;
  font-style: normal;
  text-align: center;
  line-height: 0.88rem;
}
.dataTime .time span.now i {
  color: #48bebb;
}
.footer {
  height: 0.88rem;
}
.footer a span.mui-icon {
  width: 0.42rem;
  height: 0.42rem;
}
.footer a span.oberItem {
  background: url(../imgs/observation/eye_gray.png) no-repeat center / 0.42rem 0.42rem;
}
.footer a span.bedOper {
  background: url(../imgs/observation/operate_gray.png) no-repeat center / 0.42rem 0.42rem;
}
.footer a span.out {
  background: url(../imgs/observation/out_gray.png) no-repeat center / 0.42rem 0.42rem;
}
.footer a span.in {
  background: url(../imgs/observation/in_gray.png) no-repeat center / 0.42rem 0.42rem;
}
.footer a span.life {
  background: url(../imgs/observation/lift_gray.png) no-repeat center / 0.42rem 0.42rem;
}
.footer a span.mui-tab-label {
  font-size: 0.24rem;
  color: #999;
}
.footer .active span.mui-tab-label {
  font-size: 0.24rem;
  color: #48bebb;
}
.footer .active .oberItem {
  background: url(../imgs/observation/eye.png) no-repeat center / 0.42rem 0.42rem;
}
.footer .active .bedOper {
  background: url(../imgs/observation/operate.png) no-repeat center / 0.42rem 0.42rem;
}
.footer .active .out {
  background: url(../imgs/observation/out.png) no-repeat center / 0.42rem 0.42rem;
}
.footer .active .in {
  background: url(../imgs/observation/in.png) no-repeat center / 0.42rem 0.42rem;
}
.footer .active .life {
  background: url(../imgs/observation/lift.png) no-repeat center / 0.42rem 0.42rem;
}
