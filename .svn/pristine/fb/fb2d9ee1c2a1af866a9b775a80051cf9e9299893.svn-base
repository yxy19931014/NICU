/*主体容器*/
.obervationBox .listcontainer .temperature,
.obervationBox .listcontainer .blood,
.obervationBox .listcontainer .Model,
.obervationBox .listcontainer .weight {
  width: 100%;
  margin-top: 0.22rem;
  border-top: 1px solid #bebebe;
  padding: 0 0.2rem 0 0.2rem;
  background: #fff;
  border-bottom: 1px solid #bebebe;
}
.obervationBox .listcontainer .temperature ul li,
.obervationBox .listcontainer .blood ul li,
.obervationBox .listcontainer .Model ul li,
.obervationBox .listcontainer .weight ul li {
  width: 100%;
  height: 0.88rem;
  background: #fff;
  border-bottom: 1px solid #bebebe;
  overflow: hidden;
  position: relative;
}
.obervationBox .listcontainer .temperature ul li i,
.obervationBox .listcontainer .blood ul li i,
.obervationBox .listcontainer .Model ul li i,
.obervationBox .listcontainer .weight ul li i {
  width: 40%;
  height: 100%;
  position: absolute;
  top: 0;
  right: 5%;
  line-height: 0.88rem;
  font-style: normal;
  color: #FF5053;
  background: #fff;
  display: none;
}
.obervationBox .listcontainer .temperature ul li span,
.obervationBox .listcontainer .blood ul li span,
.obervationBox .listcontainer .Model ul li span,
.obervationBox .listcontainer .weight ul li span {
  width: 30%;
  height: 0.88rem;
  float: left;
  font-size: 0.28rem;
  color: #333;
  line-height: 0.88rem;
}
.obervationBox .listcontainer .temperature ul li input,
.obervationBox .listcontainer .blood ul li input,
.obervationBox .listcontainer .Model ul li input,
.obervationBox .listcontainer .weight ul li input {
  width: 70%;
  height: 0.86rem;
  font-size: 0.28rem;
  float: left;
  text-align: right;
  border: none;
  padding: 0;
  padding-right: 0.2rem;
  margin-bottom: 0;
  color: #999;
}
.obervationBox .listcontainer .temperature ul li input.testName,
.obervationBox .listcontainer .blood ul li input.testName,
.obervationBox .listcontainer .Model ul li input.testName,
.obervationBox .listcontainer .weight ul li input.testName {
  width: 65%;
}
.obervationBox .listcontainer .temperature ul li span.unit,
.obervationBox .listcontainer .blood ul li span.unit,
.obervationBox .listcontainer .Model ul li span.unit,
.obervationBox .listcontainer .weight ul li span.unit {
  width: 5%;
}
.obervationBox .listcontainer .temperature ul .selectItem span,
.obervationBox .listcontainer .blood ul .selectItem span,
.obervationBox .listcontainer .Model ul .selectItem span,
.obervationBox .listcontainer .weight ul .selectItem span {
  width: 50%;
}
.obervationBox .listcontainer .temperature ul .selectItem .selectRight,
.obervationBox .listcontainer .blood ul .selectItem .selectRight,
.obervationBox .listcontainer .Model ul .selectItem .selectRight,
.obervationBox .listcontainer .weight ul .selectItem .selectRight {
  text-align: right;
  padding-right: 0.2rem;
  color: #999;
}
.obervationBox .listcontainer .temperature ul .lastLi,
.obervationBox .listcontainer .blood ul .lastLi,
.obervationBox .listcontainer .Model ul .lastLi,
.obervationBox .listcontainer .weight ul .lastLi {
  border: none;
}
.obervationBox .listcontainer h3 {
  width: 100%;
  height: 0.88rem;
  font-size: 0.28rem;
  line-height: 0.88rem;
  padding: 0;
  margin: 0;
  padding-left: 0.2rem;
  font-weight: 400;
}
.obervationBox .listcontainer .Model {
  margin-top: -0.05rem;
}
.obervationBox .listcontainer .last {
  margin-bottom: 1rem;
}
.obervationBox .listcontainer .first {
  background: #48bebb;
}
.obervationBox .listcontainer .saveBack {
  width: 100%;
  background: #efefef;
  position: fixed;
  bottom: 0;
  display: flex;
  padding-left: 0.1rem;
}
.obervationBox .listcontainer .save {
  width: 3.5rem;
  height: 0.7rem;
  background: #48bebb;
  margin-left: 0.1rem;
  margin-bottom: 0.2rem;
  color: #fff;
  font-size: 0.28rem;
  font-weight: bold;
}
.obervationBox .listcontainer .clearButton {
	width: 3.5rem;
  height: 0.7rem;
  background: #ccc;
  margin-left: 0.1rem;
  margin-bottom: 0.2rem;
  color: #fff;
  font-size: 0.28rem;
  font-weight: bold;
}
.obervationBox .listcontainer .clearButton.clearAble {
	background: #48bebb;
}
.obervationBox .listcontainer.hasSave .temperature ul input,
.obervationBox .listcontainer.hasSave .blood ul input,
.obervationBox .listcontainer.hasSave .weight ul input,
.obervationBox .listcontainer.hasSave .Model ul input {
  color: #333;
}
.obervationBox .listcontainer.hasSave .temperature ul input::-webkit-input-placeholder,
.obervationBox .listcontainer.hasSave .blood ul input::-webkit-input-placeholder,
.obervationBox .listcontainer.hasSave .weight ul input::-webkit-input-placeholder,
.obervationBox .listcontainer.hasSave .Model ul input::-webkit-input-placeholder {
  /* placeholder颜色  */
  color: #333;
}
.obervationBox .listcontainer.hasSave .temperature .selectItem .selectRight,
.obervationBox .listcontainer.hasSave .blood .selectItem .selectRight,
.obervationBox .listcontainer.hasSave .weight .selectItem .selectRight,
.obervationBox .listcontainer.hasSave .Model .selectItem .selectRight {
  color: #333;
}
.obervationBox .listcontainer.hasSave.OtherData .temperature ul input,
.obervationBox .listcontainer.hasSave.OtherData .blood ul input,
.obervationBox .listcontainer.hasSave.OtherData .weight ul input,
.obervationBox .listcontainer.hasSave.OtherData .Model ul input {
	color: #999;
}
.obervationBox .listcontainer.hasSave.OtherData .temperature ul input::-webkit-input-placeholder,
.obervationBox .listcontainer.hasSave.OtherData .blood ul input::-webkit-input-placeholder,
.obervationBox .listcontainer.hasSave.OtherData .weight ul input::-webkit-input-placeholder,
.obervationBox .listcontainer.hasSave.OtherData .Model ul input::-webkit-input-placeholder {
  /* placeholder颜色  */
  color: #999;
}
.obervationBox .listcontainer.hasSave.OtherData .temperature .selectItem .selectRight,
.obervationBox .listcontainer.hasSave.OtherData .blood .selectItem .selectRight,
.obervationBox .listcontainer.hasSave.OtherData .weight .selectItem .selectRight,
.obervationBox .listcontainer.hasSave.OtherData .Model .selectItem .selectRight {
  color: #999;
}
