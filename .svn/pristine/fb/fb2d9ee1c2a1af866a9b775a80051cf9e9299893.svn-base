.selectArea .selectInput {
	width: 100%;
	height: 0.8rem;
	border:none;
	color:#48bebb;
	margin: 0;
	border-radius: 0;
	padding: 0;
	padding-left: 0.2rem;
	border-bottom: 2px solid #ccc;
}
